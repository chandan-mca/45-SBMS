package in.ashokit.dao;

import org.springframework.stereotype.Component;

@Component
public class UserDao {

	public UserDao() {
		System.out.println("UserDao  :: Constructor");
	}

	public String findUsernameById(int userId) {
		if (userId == 1) {
			return "Raja";
		} else if (userId == 2) {
			return "Rani";
		} else {
			return "Invalid User Id";
		}
	}

}
