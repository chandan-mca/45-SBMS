package in.ashokit;

public class Car {
	
	public Car() {
		System.out.println("Car - Constructor Executed...");
	}
	
	public void drive() {
		
		Engine engine = new Engine();
		int engStatus =  engine.start();
		
		if(engStatus == 1) {
			System.out.println("Journey Started...");
		} else {
			System.out.println("Engine Problem...");
		}
	}
}
